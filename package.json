{
  "name": "cpm",
  "version": "0.0.0",
  "description": "Cosmic Package Manager",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && broccoli build dist; chmod -f u+x dist/bin/*; rm -rf tmp",
    "docs": "yuidoc .",
    "lint": "eslint . || exit 0",
    "prepublish": "npm run build && npm run docs",
    "test": "npm run build && mocha ./dist/test -R spec"
  },
  "bin": {
    "cpm": "dist/bin/cpm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rosshadden/cpm.git"
  },
  "keywords": [
    "cosmic",
    "package manager",
    "universal",
    "language-agnostic"
  ],
  "author": "Ross Hadden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rosshadden/cpm/issues"
  },
  "homepage": "https://github.com/rosshadden/cpm#readme",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "toml": "^2.3.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.5.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-funnel": "^1.0.3",
    "broccoli-merge-trees": "^1.1.2",
    "broccoli-static-compiler": "^0.2.2",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-zipscene": "^0.1.9",
    "mocha": "^2.5.3",
    "source-map-support": "^0.4.2",
    "yuidocjs": "^0.10.2"
  }
}
